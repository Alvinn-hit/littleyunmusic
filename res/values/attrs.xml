<?xml version="1.0" encoding="utf-8"?>
<resources>
    <attr name = "color_textcolor" format="color"></attr>
    <attr name = "bg_normal_color" format="color"></attr>    
    <declare-styleable name = "CircleImageView">
        <attr name = "stroke_width" format="dimension"></attr>
        <attr name = "stroke_color" format="color"></attr>
        <attr name = "content_imageview" format="reference"></attr>
        <attr name = "roundrect_imageview" format="boolean"></attr>
        <attr name = "iscolorfilter" format="boolean"></attr>
        <attr name = "round_radius" format="dimension"></attr>
        <attr name = "alpha" format="integer"></attr>
        <attr name = "text" format="string"></attr>
        <attr name = "padding" format="dimension"></attr>
        <attr name = "text_size" format="dimension"></attr>
    </declare-styleable>
    <declare-styleable name = "LintEdt">
   		<attr name="drawableleftunfocused" format="reference"></attr>
   		<attr name="drawableleftfocused" format="reference"></attr>
   	</declare-styleable>
   	<declare-styleable name = "TriangleView">
   		<attr name="strokewidth" format="dimension"></attr>
   	</declare-styleable>
   	
   	
   	    <attr name="cropImageStyle" format="reference" />

    <declare-styleable name="CropImageView">
        <attr name="highlightColor" format="reference|color" />
        <attr name="showThirds" format="boolean" />
        <attr name="showCircle" format="boolean" />
        <attr name="showHandles">
          <enum name="changing" value="0" />
          <enum name="always" value="1" />
          <enum name="never" value="2" />
        </attr>
    </declare-styleable>
</resources>
